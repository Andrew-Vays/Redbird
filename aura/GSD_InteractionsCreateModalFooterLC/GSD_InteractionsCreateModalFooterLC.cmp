<aura:component >
    <aura:registerEvent name="saveModalEvent" type="c:GSD_saveModalEventLE"/>
    <aura:handler event="c:GSD_closeModalEventLE" action="{!c.closeModalEvent}"/>
    
    <lightning:overlayLibrary aura:id="overlayLib"/>
    <lightning:button name="cancel" label="Cancel" onclick="{!c.handleCancel}"/>
    <lightning:button name="ok" label="OK" variant="brand" onclick="{!c.handleOK}"/>
 </aura:component>